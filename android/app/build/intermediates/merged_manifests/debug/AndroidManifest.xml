        <meta-data
            android:name="com.huawei.hms.client.service.name:base"
            android:value="base:6.2.0.300" /> <!-- SDK依赖的HMSCore的最低api level元数据 -->
        <meta-data
            android:name="com.huawei.hms.min_api_level:base:hmscore"
